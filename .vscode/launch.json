{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Firefox",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/dist",
      "pathMappings": [
        {
          "url": "webpack:///",
          "path": "${workspaceFolder}"
        }
      ]
    }
  ]
}
